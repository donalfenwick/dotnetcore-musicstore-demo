{
  "compilerOptions": {
    "outDir": "./wwwroot/js/compiled",
    "target": "es5",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noEmitOnError": true,
    "allowSyntheticDefaultImports": true,
    "removeComments": false,
    "sourceMap": true,
    "strict": true
  },
  "include": [
    "./typescript"
  ]
}