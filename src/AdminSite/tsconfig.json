{
  "compilerOptions": {
    "outFile": "./wwwroot/js/compiled/app-libs.js",
    "target": "es5",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noEmitOnError": true,
    "allowSyntheticDefaultImports": true,
    "removeComments": false,
    "sourceRoot": "./typescript/",
    "sourceMap": true,
    "strict": true
  },
  "include": [
    "./typescript"
  ]
}